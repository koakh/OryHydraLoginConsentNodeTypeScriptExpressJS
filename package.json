{
  "name": "hydra-login-consent-logout",
  "version": "0.0.0",
  "bin": {
    "hydra-login-consent-logout": "lib/app.js"
  },
  "prettier": "ory-prettier-styles",
  "config": {
    "prettierTarget": "{src,views}/{*,**/*}.{ts,js,json}"
  },
  "scripts": {
    "build": "tsc",
    "serve": "node lib/app.js",
    "start": "PORT=3001 HYDRA_ADMIN_URL=http://localhost:4445 ts-node-dev --watch public,views --respawn src/app.ts",
    "test": "npm-run-all build",
    "lint": "eslint . --ext .ts",
    "twind": "npx tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "docker:build": "docker build -t hydra-login-consent-logout ."
  },
  "dependencies": {
    "@oryd/hydra-client": "^1.9.0-alpha.2",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.6",
    "pug": "^3.0.2",
    "querystring": "^0.2.1",
    "serve-favicon": "^2.5.0",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "npm-run-all": "^4.1.5",
    "ory-prettier-styles": "^1.2.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.3",
    "ts-node-dev": "^2.0.0"
  }
}
